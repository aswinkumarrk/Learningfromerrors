#include <iostream>

using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    string s1,s2;
	    int total=0;
	    cin>>s1>>s2;
	    for(int i=0;i<s1.length();i++)
	    {
	        for(int j=0;j<s1.length();j++)
	        {
	            if(s1[i]==s2[j])
	            {
	            total=total+1;
	            break;
	            
	            }
	        }
	        
	        
	    }
	
	   
	    cout<<total;
	    if(total==s1.length())
	    {
	        cout<<"YES"<<endl;
	    }
	    else
	    {
	        cout<<"NO"<<endl;
	    }
	}
	    

}
