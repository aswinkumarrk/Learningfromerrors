// Write your code here
#include<iostream>
using namespace std;

int main() {
	string s;
	cin >>s;										// Reading input from STDIN
	for(int i=0;i<s.length();i++)
	{
	    if((s[i]+s[i+1])%2 == 0)
	    {
	        if(s[2] != 'A' || s[2] != 'E' )
	        cout<<"valid";
	        return 0;
	       
	    }
	    	else
	    {
	    cout<<"invalid";
	    return 0;
	    }
	    
	    
	    
	}

}
