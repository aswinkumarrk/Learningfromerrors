
#include <iostream>

using namespace std;

int main()
{
    int n;
    string s;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>s[i];
    }
    for(int i=0;i<n;i++)
    {
        if(s[i]=='.')
        {
            s[i]='B';
            
            
        }
    }
    
    cout<<"after . changes to B "<<s;
    for(int i=0;i<n;i++)
    {
        cout<<s[i];
    }
    

    return 0;
}
