// Sample code to perform I/O:

#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main() {
	 int X,Y;
     string A;
	cin>>A;
	for(int i=0;i<A.size();i++)
	{
	    if(A[i]=='L')
	    {
	        X=X-1;
	        
	    }
	    if(A[i]=='R')
	    {
	        X=X+1;
	        
	    }
	    if(A[i]=='D')
	    {
	        Y=Y-1;
	    }
	    if(A[i]=='U')
	    {
	        Y=Y+1;
	        
	    }
	    
	}
	cout<<X<<" "<<Y<<endl;

    
}

