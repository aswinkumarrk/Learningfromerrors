#include <iostream>
 
using namespace std;
 
int main() {
	long long int n,q,a,b;
	
	cin>>n>>q;										// Reading input from STDIN
    int arr[n];
    for(int i=1;i<=n;i++)
    {
        cin>>arr[i];
    }
    while(q--)
    {
     int t=0;
	 int s=0;
    cin>>a>>b;
    for(int i=a;i<=b;i++)
    {
        t=t+1;
        s=s+arr[i];
    }
    int ans=s/t;
    cout<<ans<<endl;
    }
}
